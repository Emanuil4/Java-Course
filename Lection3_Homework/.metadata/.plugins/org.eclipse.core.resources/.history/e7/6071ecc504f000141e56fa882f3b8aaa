package homeworkLection3;
import java.util.Scanner;
public class numberOnEnglish {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in,"UTF-8");
		System.out.print("Please enter a number:");
       int  a = input.nextInt();
       String ch="ty";
       if (a >= 0 && a <=9){
    	   switch(a){
           case 1:
        	   System.out.print("One");
        	   break;
           case 2:
        	   System.out.print("Two");
        	   break;
           case 3:
        	   System.out.print("Three");
        	   break;
           case 4:
        	   System.out.print("Four");
        	   break;
           case 5:
        	   System.out.print("Five");
        	   break;
           case 6:
        	   System.out.print("Six");
        	   break;
           case 7:
        	   System.out.print("Seven");
        	   break;
           case 8:
        	   System.out.print("Eight");
        	   break;
           case 9:
        	   System.out.print("Nine");
        	   break;
       }
	} if(a==10){
		System.out.print("Ten");
	}if (a==11){
		System.out.print("Eleven");
	}if(a==12){
		System.out.print("Twelve");
	}if(a>12 && a<=19){
		switch(a){
	
	       case 1:
	    	   System.out.print("Thir");
	    	   System.out.print(ch);
	    	   break;
	       case 2:
	    	   System.out.print("four");
	    	   System.out.print(ch);
	    	   break;
	       case 3:
	    	   System.out.print("Fif");
	    	   System.out.print(ch);
	    	   break;
	       case 4:
	    	   System.out.print("Four");
	    	   System.out.print(ch);
	    	   break;
	       case 5:
	    	   System.out.print("Fif");
	    	   System.out.print(ch);
	    	   break;
	       case 6:
	    	   System.out.print("Six");
	    	   System.out.print(ch);
	    	   break;
	       case 7:
	    	   System.out.print("Seven");
	    	   System.out.print(ch);
	    	   break;
	       case 8:
	    	   System.out.print("Eigh");
	    	   System.out.print(ch);
	    	   break;
	       case 9:
	    	   System.out.print("Nine");
	    	   System.out.print(ch);
	    	   break;
	}
	}	
	}
}
	

