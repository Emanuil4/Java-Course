public class from1to30 {
	public static void main(String[] args) {
		int row;
		int col;
	 int[][] mas1= new int[5][6];
	 for(row= 0; row < mas1.length; row++){
		 for(col = 0; col < mas1[row].length ; col++){
			 for (int i = 30; i > 0; i--){
				 mas1[row][col] = i;
				 i--;
			 }
			 System.out.print(mas1[row][col] + " ");
			}
		 System.out.println();
	 }
	}
		} 
	
