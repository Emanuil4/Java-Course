public class from1to30 {
	public static void main(String[] args) {
		int row = 5;
		int col = 6;
	 int[][] mas1= new int[row][col];
	 for(int rows= 0; rows < 5; rows++){
		 for( int cols = 0; col < 6 ; cols++){
			 for (int i = 30; i >= 1; i--){
				 mas1[rows][cols] = i;
			 }
			 System.out.print(mas1[rows][cols] + " ");
			}
		 System.out.println();
	 }
		} 
	}
