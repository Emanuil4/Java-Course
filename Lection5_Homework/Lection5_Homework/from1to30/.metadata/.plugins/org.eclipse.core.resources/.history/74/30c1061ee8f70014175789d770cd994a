public class from1to30 {
	public static void main(String[] args) {
		int row = 5;
		int col = 6;
	 int[][] mas1= new int[row][col];
	 for (int i = 30; i >= 1; i--){
		 mas1[row][col] = i;
	 
	 for(row= 0; row < mas1.length; row++){
		 for(col = 0; col < mas1[row].length ; col++){
			 System.out.print(mas1[row][col] + " ");
	 }
		 System.out.println();
		} 
	}
}
	}