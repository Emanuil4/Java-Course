
public class NK_ {

	public static void main(String[] args) {
		int N=4;
		int K=3;
		long factorial = 1;
		for (int i = 1; i <= N; i++) {
		    factorial *= i;
		} 
		   System.out.println(N + "! = " + factorial);
		long factorial1 = 1;
		for (int i = 1; i <= K; i++) {
		    factorial1 *= i;
		} 
		   System.out.println(K + "! = " + factorial1);
		  

	}

}
