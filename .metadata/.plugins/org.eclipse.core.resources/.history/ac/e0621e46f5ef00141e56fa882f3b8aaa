package homeworkLection3;
import java.util.Scanner;
public class game {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in,"UTF-8");
		System.out.print("Please enter number from 1 to 9: ");
		int a = input.nextInt();
		if ( (a < 1) && (a > 9) ){
			System.out.print("Please enter a number form 1 to 9");
		}
		if ( (a >= 1) && (a <= 3 ) ){
			int d = a*5;
			System.out.print(d);
		}if( (a>=4) && (a<=6) ){
			int c= a*10;
			System.out.print(c);
		}if ( (a>=7) && (a<=9) ){
			int f= a*50;
			System.out.print(f);
		}
	}

}
