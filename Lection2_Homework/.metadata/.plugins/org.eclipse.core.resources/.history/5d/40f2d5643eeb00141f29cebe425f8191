
public class swapValues {

	public static void main(String[] args) {
		int a=1;
		int b=3;
		 int c=a;
		 System.out.print("a=");
		 System.out.println(a);
		 System.out.print("b=");
		 System.out.println(b);
		 a=b;
		 b=c;
		 System.out.print("a=");
		 System.out.print(a);
		 System.out.print(" b=");
		 System.out.println(b);
		 

	}

}
